      STATUS current
      DESCRIPTION
        "The table of ATM interface statistic, it is used for querying Tx/Rx number of cells.
         The index of this table is ifIndex."
      ::= { hwmusaAtm 101 }
    
    hwmusaAtmStatisticEntry OBJECT-TYPE
      SYNTAX HwmusaAtmStatisticEntry
      MAX-ACCESS not-accessible
      STATUS current
      DESCRIPTION
        "The table of ATM interface statistic, it is used for querying Tx/Rx number of cells.
         The index of this entry is ifIndex."
      INDEX { ifIndex }
      ::= { hwmusaAtmStatisticTable 1 }

    HwmusaAtmStatisticEntry ::=
      SEQUENCE 
      { 
        hwmusaAtmStatisticTxCellSucc
          Gauge32,
        hwmusaAtmStatisticRxCellSucc
          Gauge32,
        hwmusaAtmStatisticRowStatus
