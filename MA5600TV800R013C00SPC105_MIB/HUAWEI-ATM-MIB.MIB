        hwmusaAtmInterfaceAlsTxOnTime
                Integer32,
        hwmusaAtmInterfaceAlsManualTxOnTime
                Integer32,
        hwmusaAtmInterfaceMaxVpiNo
                Integer32,
        hwmusaAtmInterfaceMinVpiNo
                Integer32,
        hwmusaAtmInterfaceMaxVciNo
                Integer32,
        hwmusaAtmInterfaceMinVciNo
                Integer32,        
        hwmusaAtmInterfacePortMode
                INTEGER,
        hwmusaAtmInterfaceUNINNIMode
                INTEGER,
        hwmusaAtmInterfaceInTotalBandWidth
                Integer32,
        hwmusaAtmInterfaceOutTotalBandwidth
                Integer32,
        hwmusaAtmInterfaceTxClock
                INTEGER,
        hwmusaAtmInterfaceEventInsert
                Integer32,
        hwmusaAtmInterfaceBipInsert
                INTEGER
      }

    hwmusaAtmInterfaceUpcNpcFlag OBJECT-TYPE
      SYNTAX      INTEGER 
      {
      enabled(1),
      disabled(2)
      }
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION 
        "The flag of UPC/NPC interface.
         Options:
         1. enabled(1)  -the flag of UPC/NPC interface is enabled 
         2. disabled(2) -the flag of UPC/NPC interface is disabled
        "
      DEFVAL { enabled }
      ::= { hwmusaAtmInterfaceExtEntry 1 }

    hwmusaAtmInterfaceAlarmStatus OBJECT-TYPE
      SYNTAX      Integer32
      MAX-ACCESS  read-only
      STATUS      current
      DESCRIPTION     
        "The alarm status of atm interface physical layer.
         The meaning of values:
         oof(0)      -out of frame    
         lof(1)      -loss of frame   
         los(2)      -loss of signal  
         lrdi(3)     -line remote defect indication   
         lais(4)     -line alarm indication signal    
         lop(5)      -loss of pointer (not supported currently)
         pais(6)     -path alarm indication signal    
         prdi(7)     -path remote defect indication   
         psbf(8)     -protection switching byte failure  
         sbip(9)     -section BIP     
         sbipe(10)   -section BIP exceeding threshold (not supported currently)
         lbip(11)    -line BIP        
         lbipe(12)   -line BIP exceeding threshold (not supported currently)
         lfebe(13)   -line far end block error        
         lfebee(14)  -line far end block error exceeding threshold (not supported currently)
         pbip(15)    -path BIP        
         pbipe(16)   -path BIP exceeding threshold (not supported currently)
         pfebe(17)   -path far end block error        
         pfebee(18)  -path far end block error exceeding threshold (not supported currently)
         hcs(19)     -head check sequence             
         chcs(20)    -correctable head check sequence 
         uhcs(21)    -uncorrectable head check sequence 
         lcd(22)     -loss of cell delineation        
         ocd(23)     -out of cell delineation         
         aprdi(24)   -auxiliary path remote defect indication (not supported currently)
         ber(25)     -bit error rate (not supported currently) 
        "
      ::= { hwmusaAtmInterfaceExtEntry 2 }

    hwmusaAtmInterfaceVcSubSpace OBJECT-TYPE
      SYNTAX      Integer32
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION 
        "The vpi number of setting vc.
         The value ranges from 1 to 256 and is an integer power of 2 in MA5600T product.
         The default 16 in MA5600T product.
        "
      DEFVAL { 256 }
      ::= { hwmusaAtmInterfaceExtEntry 3 }

    hwmusaAtmInterfaceMyNeighborIfIndex OBJECT-TYPE
      SYNTAX      InterfaceIndex
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION 
        "The neighbor system ifIndex. "
        ::= { hwmusaAtmInterfaceExtEntry 4 }

    hwmusaAtmInterfaceInCurBandWidth OBJECT-TYPE
      SYNTAX      Integer32
      MAX-ACCESS  read-only
      STATUS      current
      DESCRIPTION 
