    hwmusaAtmInterfaceTxClock OBJECT-TYPE
      SYNTAX INTEGER 
      {
      system(1),
      line(2),
      invalid(-1)
      }
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
        "The transmit clock type of the interface.
         Options:
         1. system(1)     -system transmit clock
         2. line(2)       -line transmit clock
         3. invalid(-1)   -invalid value
         "
      ::= { hwmusaAtmInterfaceExtEntry 109 }
      
    hwmusaAtmInterfaceEventInsert OBJECT-TYPE
      SYNTAX Integer32 (0..31)
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
        "Insert or cancel alarm event in the interface.
         The value is bit map, set the bit can insert the event, clear the bit can cancel the event.
         The various bit positions are:
         1   -LAIS
         2   -LOF  
         4   -LOS
         8   -LRDI
         16  -PAIS"
      ::= { hwmusaAtmInterfaceExtEntry 110 }
      
    hwmusaAtmInterfaceBipInsert OBJECT-TYPE
      SYNTAX INTEGER 
      {
      lbip(1),
      pbip(2),
      sbip(3)
      }
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
        "Insert BIP error code type in the interface.
         Options:
         1. lbip(1)   -L-bip
         2. pbip(2)   -P-bip
         3. sbip(3)   -S-bip
         "
      ::= { hwmusaAtmInterfaceExtEntry 111 }

    hwmusaAtmStatisticTable OBJECT-TYPE
