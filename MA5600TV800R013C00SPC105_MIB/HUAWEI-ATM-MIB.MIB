      MAX-ACCESS read-create
      STATUS current
      DESCRIPTION
        "The present operation status of the row.
         Options:
         1. active(1),        -the status of the row is active 
         2. notInService(2),  -the status of the row is notInService
         3. notReady(3),      -the status of the row is notReady
         4. createAndGo(4),   -the status of the row is createAndGo
         5. createAndWait(5), -the status of the row is createAndWait
         6. destroy(6)        -the status of the row is destroy
         "
      ::= { hwmusaAtmStatisticEntry 3 }
          
    hwAtmECSMonitorControlTable OBJECT-TYPE
      SYNTAX      SEQUENCE OF HwAtmECSMonitorControlEntry
      MAX-ACCESS  not-accessible
      STATUS      current
      DESCRIPTION
        "
        perform monitor status control table
        --STM-1/STM-4 Control table of the enabling state of the transmission performance monitoring
        The indexes of this table are ifIndex and hwAtmECSMonitorPeriodType.
        "
      ::= { hwmusaAtm 102 }

    hwAtmECSMonitorControlEntry OBJECT-TYPE
      SYNTAX      HwAtmECSMonitorControlEntry
      MAX-ACCESS  not-accessible
      STATUS      current
      DESCRIPTION
        "
        perform monitor status control table
        --STM-1/STM-4 Control table of the enabling state of the transmission performance monitoring
        The indexes of this entry are ifIndex and hwAtmECSMonitorPeriodType.
        "
      INDEX { 
              ifIndex,
              hwAtmECSMonitorPeriodType
            }
      ::= { hwAtmECSMonitorControlTable 1 }

    HwAtmECSMonitorControlEntry ::=
      SEQUENCE
      {
       hwAtmECSMonitorPeriodType                   INTEGER,
       hwAtmECSMonitorStatusReal                   INTEGER,
       hwAtmECSMonitorStartTimeReal                DateAndTime,     
       hwAtmECSMonitorStatusUserSet                INTEGER,
       hwAtmECSMonitorStartTimeUserSet             DateAndTime             
      }
      
    hwAtmECSMonitorPeriodType OBJECT-TYPE
      SYNTAX      INTEGER{quarter(1),day(2)}
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
          "
          the period of the monitor, 1:quarter, 2:day.
          The monitoring periods of the bit error statistics of the SDH transmission performance, which are 15 minutes and 24 hours.
          Options:
          1. quarter(1)   -The quarter period of the monitor 
          2. day(2)       -The day period of the monitor
          "
      ::={ hwAtmECSMonitorControlEntry 1 }
    
    hwAtmECSMonitorStatusReal OBJECT-TYPE
      SYNTAX      INTEGER{disable(0),enable(1)}
      MAX-ACCESS  read-only
      STATUS      current
      DESCRIPTION
          "
          current monitor status, 0:disable, 1:enable.
          The actual states of the monitoring function of the SDH transmission performance bit error statistics, which are enable and disable.
          DEFVAL {enable}
          "
    
      ::={ hwAtmECSMonitorControlEntry 2 }

    hwAtmECSMonitorStartTimeReal OBJECT-TYPE
      SYNTAX      DateAndTime
      MAX-ACCESS  read-only
      STATUS      current
      DESCRIPTION
          "
          The actual start time of the monitoring period of the SDH transmission performance bit error statistics. 
          "
      ::={ hwAtmECSMonitorControlEntry 3 }
    
    hwAtmECSMonitorStatusUserSet OBJECT-TYPE
      SYNTAX      INTEGER{disable(0),enable(1)}
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
          "
          The actual state of the monitoring function of the SDH transmission performance bit error statistics set by the user, which is enable or disable.
          "
      DEFVAL {1}
      ::={ hwAtmECSMonitorControlEntry 4 }

    hwAtmECSMonitorStartTimeUserSet OBJECT-TYPE
      SYNTAX      DateAndTime
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
          "
          The time of enabling or disabling the periodical monitoring of the SDH transmission performance bit error statistics set by the user.
          "
      ::={ hwAtmECSMonitorControlEntry 5 }

--STM-1/STM-4 Control table of the enabling state of the transmission performance monitoring threshold
      
    hwAtmECSThresholdControlTable OBJECT-TYPE
      SYNTAX      SEQUENCE OF HwAtmECSThresholdControlEntry
      MAX-ACCESS  not-accessible
      STATUS      current
      DESCRIPTION
        "
        STM-1/STM-4 threshold enable table
        --STM-1/STM-4 Control table of the enabling state of the transmission performance monitoring.
        The indexes of this table are ifIndex, hwAtmECSMonitorObjType and hwAtmECSMonitorPeriodType.
        "
      ::= { hwmusaAtm 103 }

    hwAtmECSThresholdControlEntry OBJECT-TYPE
      SYNTAX      HwAtmECSThresholdControlEntry
      MAX-ACCESS  not-accessible
      STATUS      current
      DESCRIPTION
        "
        STM-1/STM-4 threshold enable table
        --STM-1/STM-4 Control table of the enabling state of the transmission performance monitoring.
        The indexes of this entry are ifIndex, hwAtmECSMonitorObjType and hwAtmECSMonitorPeriodType.
        "
      INDEX { 
              ifIndex,
              hwAtmECSMonitorObjType,
              hwAtmECSMonitorPeriodType
            }
      ::= { hwAtmECSThresholdControlTable 1 }
      
    HwAtmECSThresholdControlEntry ::=
      SEQUENCE
      {
       hwAtmECSMonitorObjType                 INTEGER,
       hwAtmECSThresholdEnabled               INTEGER
      
      }
      
    hwAtmECSMonitorObjType OBJECT-TYPE
      SYNTAX      INTEGER{rs(0),ms(1),hp(2)}
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
          "
           the section of the perform monitor,
           Options:
           1. rs(0)       -The regenerator section
           2. ms(1)       -The multiplex section
           3. hp(2)       -The higher order path 
           
           The monitoring objects of the SDH transmission performance bit error statistics, which are the regenerator section, multiplex section, and higher order path
          "
      ::={ hwAtmECSThresholdControlEntry 1 }
      
    hwAtmECSThresholdEnabled OBJECT-TYPE
      SYNTAX      INTEGER{disable(0),enable(1)}
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
          "
           SDH threshold enable
           The enabling state of the monitoring threshold of the SDH transmission performance bit error statistics
          "
      DEFVAL {1}
      ::={ hwAtmECSThresholdControlEntry 2 }
          
          
--The monitoring threshold table of the STM-1/STM-4 transmission performance bit error statistics of the ATM port
      
